[
  {
    "key": "cmd+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorIsOpen"
  },
  {
    "key": "ctrl+cmd+r",
    "command": "workbench.files.action.refreshFilesExplorer"
  },
  {
    "key": "cmd+shift+e",
    "command": "workbench.files.action.showActiveFileInExplorer",
    "when": "editorTextFocus"
  }
]
